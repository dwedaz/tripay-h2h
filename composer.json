{
    "name": "dwedaz/tripay-h2h",
    "description": "Laravel package for Tripay H2H PPOB API integration",
    "type": "library",
    "keywords": [
        "laravel",
        "tripay",
        "ppob",
        "payment",
        "pulsa",
        "tagihan"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dwedaz",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/container": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0",
        "mockery/mockery": "^1.4.4"
    },
    "autoload": {
        "psr-4": {
            "Tripay\\H2H\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tripay\\H2H\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tripay\\H2H\\Providers\\TripayServiceProvider",
                "Tripay\\H2H\\Providers\\TripayBackpackServiceProvider"
            ],
            "aliases": {
                "Tripay": "Tripay\\H2H\\Facades\\Tripay"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
